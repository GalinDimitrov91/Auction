package com.dxc.auction.core;

/**
 * Interface converter from Web UI form to model.
 * 
 * @author gdimitrov5
 *
 * @param <Form>
 * @param <Model>
 */
public interface Converter<Form, Model> {

	/**
	 * Convert from model to form.
	 * 
	 * @param model
	 * @return Form
	 */
	Form convertToForm(final Model model);

	/**
	 * Convert from form to model.
	 * 
	 * @param form
	 * @return Model
	 */
	Model convertToModel(final Form form);
}
